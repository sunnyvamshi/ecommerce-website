@model List<Company>
<partial name="_Notification"/>


<table id="tblsData" class="table table-bordered table-striped">
	<div class="container pt-4 pb-3">
		<div class="row">
			<div class="col-md-6">
				
				<h1> Company List</h1>

			</div>
			<div class="col-md-6">
				<a asp-controller="Company" asp-action="Create"
				  class="btn btn-dark">add a new Company  <i class="bi bi-plus-circle"></i></></a>
			</div>
		</div>
	</div>
	<thead>
		<tr>
			<th>Id</th>
			<th>Company Name</th>
			<th>door Adress</th>
			<th>City</th>
			<th>State</th>
			<th>PostalCode</th>
			<th>Phone Number</th>
			
		</tr>
	</thead>
	
</table>
@section Scripts{
	<script src="~/js/Company.js"></script>

}
